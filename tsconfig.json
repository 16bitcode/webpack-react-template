{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es6",
    "lib": ["dom", "ESNext", "DOM.Iterable"],
    "allowJs": true,
    "noImplicitAny": false,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node"
  },
  "include": ["src/**/*"]
}
